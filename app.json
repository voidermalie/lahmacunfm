{
  "expo": {
    "name": "lahmacunfm",
    "slug": "lahmacunfm",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo/lahma120.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/img/dj-citrom-web.png",
      "resizeMode": "contain",
      "backgroundColor": "#d09cf8"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#d09cf8"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "lahmacun"
  }
}
